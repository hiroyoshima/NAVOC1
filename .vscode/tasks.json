{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Export Dynamics NAV Objects",
            "type": "shell",
            "command": "${fileDirnameBasename}",
        },
        {
            "label": "Export-Nav-Objects",
            "type": "shell",
            "command": "Import-Module 'D:\\Development\\Dynamics NAV\\NAVOC1\\Export Objects.ps1'; Export-Nav-Objects -VersionList '${fileDirnameBasename}'",
        },
        {
            "label": "Export-Nav-Objects DevFob",
            "type": "shell",
            "command": "Import-Module 'D:\\Development\\Dynamics NAV\\NAVOC1\\Export Objects.ps1'; Export-Nav-Objects -VersionList '${fileDirnameBasename}' -ExportDevFob $true",
        },
        {
            "label": "Export-Nav-Objects Full",
            "type": "shell",
            "command": "Import-Module 'D:\\Development\\Dynamics NAV\\NAVOC1\\Export Objects.ps1'; Export-Nav-Objects -VersionList '${fileDirnameBasename}' -ExportDevFob $true -ExportLiveFob $true",
        },
        {
            "label": "Setup Objects",
            "type": "shell",
            "command": "Import-Module '${workspaceFolder}\\Setup.ps1'; $SecureString = ConvertTo-SecureString -String '${config:Password}' -AsPlainText -Force; Setup-NAV-Objects -DatabaseServer '172.16.1.221' -DatabasenameDev '${config:DatabaseNameDev}' -DatabaseNameLive '${config:DatabaseNameLive}' -Username '${config:Username}' -Password '${config:Password}' -ProjectPath '${workspaceFolder}' -RunAsDateExe '${config:RunAsDateExe}' -FinSqlExe '${config:FinSqlExe}'",
        },
    ]
}